
.new-assessment-name-container
{
    border: none;
    border-radius: 4px;
    bottom: auto;
    min-height: 10rem;
    left: 50%;
    padding: 2rem;
    position: fixed;
    right: auto;
    top: 50%;
    transform: translate(-50%,-50%);
    min-width: 20rem;
    width: 80%;
    max-width: 60rem;
}

body,html{
    background:rgb(241,243,243);
}
.new-assessment-name-container .new-assessment-name-title {
    text-align:center;
    font-family:"Trajan Pro";
    margin-bottom:20px;
    font-size:3em;
}
.new-assessment-name-container  .new-assessment-name-btn{
    position:relative;
    left:50%;
    transform:translateX(-50%);
    background:white;
    border:none;
    box-shadow:0px 1px 1px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
    border-radius:10px;
    padding:5px 10px;
    margin-top:20px;
    color:rgb(88, 88, 88);
    font-size:18px;
    cursor:pointer;
    font-family:"Trajan Pro";
    transition:all 500ms;
}

.new-assessment-name-container  .new-assessment-name-btn:hover{
    background:rgb(250,250,250);
    color:rgb(10,10,10);
    box-shadow:0px 0px 20px rgba(255,255,255,1),inset 0px 0px 3px rgba(0,0,0,.7);
}
.new-assessment-name-container  .new-assessment-name-btn:active{
    background:rgb(250,250,250);
    color:rgb(10,10,10);
    box-shadow:0px 0px 20px rgba(255,255,255,1),inset 0px 0px 10px rgba(0,0,0,.7);
}
.new-assessment-name-container  .new-assessment-name-input{
    text-align:center;
    outline:none;
    margin-left: 10px;
    font-size: 20px;
    border-radius:4px;
    font-family:"Adobe Caslon Pro";
    padding:0px;
    padding-top:8px;
    box-shadow:0px 1px 2px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
    border:none;
    border-color:transparent;
    transition:all 500ms;
}
.new-assessment-name-container  .new-assessment-name-input:focus{
    border:none;
    border-color:transparent;
    box-shadow:0px 1px 1px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);
}
.new-assessment-container{
    background:rgb(241,243,243);
}
.new-assessment-container .top-header-container{
    padding:10px;
    background:white;
    border-top:1px solid rgba(100,100,100,.7);
    border-bottom:1px solid rgba(100,100,100,.7);

}
.new-assessment-container .top-header-container .top-header-title{
    font-family:"Trajan Pro";
    margin-left:100px;
    font-size:23px;
    font-weight:bold;
    color:rgb(100,100,100);
    display:inline-block;
}
.new-assessment-container .top-header-container .top-header-tools{
    padding-right:100px;
    display:inline-block;
    left:100vw;
    position:absolute;
    white-space:nowrap;
    transform:translateX(-100%);
}
.new-assessment-container .top-header-container .top-header-tools .top-header-expiration-date-container:hover,
.new-assessment-container .top-header-container .top-header-tools .top-header-time-allowed-container:hover{
    border-color:rgb(180,180,180);
    background:rgb(240,240,240);
}

.new-assessment-container .top-header-container .top-header-tools .top-header-expiration-date-container,
.new-assessment-container .top-header-container .top-header-tools .top-header-time-allowed-container{
    color:rgb(100,100,100);
    display:inline-block;
    margin:0px 15px;
    font-family:"Adobe Caslon Pro";
    line-height: 15px;
    font-size:20px;
    border:2px solid transparent;
    border-radius:5px;
    padding:5px 10px;
    cursor:pointer;
    transition: all 200ms;

}
.new-assessment-container .top-header-container .top-header-tools .top-header-time-allowed-container .fa,
.new-assessment-container .top-header-container .top-header-tools .top-header-expiration-date-container .fa{
    margin-right:5px;
}
.new-assessment-container .top-header-container .top-header-tools .top-header-preview-container{
    padding:2px 10px;
    /*box-shadow:0px 1px 3px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);*/
    border:1px solid rgba(100,100,100,.7);
    font-family:"Trajan Pro";
    font-size:20px;
    border-radius:1px;
    margin:0px 10px;
    display:inline-block;
    cursor:pointer;
    transition:all 200ms;
}
.new-assessment-container .top-header-container .top-header-tools .top-header-preview-container:hover{
    background:rgb(240,240,240);
}

.new-assessment-container .build-your-assessment-container .assessment-questions-container {
    left:5%;
    width:90%;
    margin-top:5%;
    background:white;
    display:inline-block;
    position:relative;
    box-shadow:0px 1px 1px rgba(0,0,0,.7),inset 0px 1px rgba(255,255,255,.7);

    border-radius:2px;
}
.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-header-container{
    padding:10px;
    background:rgb(135,142,157);
    color:white;
}
.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-header-container .assessment-questions-header-title{
    font-size:25px;
    display:inline-block;
    font-family:"Trajan Pro";
    font-weight:bold;
}
.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-header-container .assessment-questions-header-add-container{

    display:inline-block;
    left:100%;
    position:absolute;
    transform:translateX(-100%);
    padding-right:20px;

}

.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-header-container .assessment-questions-header-add-container .btn:hover,
.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-header-container .assessment-questions-header-add-container .assessment-questions-container-add-btn:hover{
    background:rgb(250,250,250);
    color:rgb(10,10,10);
    box-shadow:0px 0px 20px rgba(255,255,255,1),inset 0px 0px 0px rgba(0,0,0,.7);
}
.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-header-container .assessment-questions-header-add-container .btn,
.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-header-container .assessment-questions-header-add-container .assessment-questions-container-add-btn{
    transition:all 300ms;
    font-size:16px;
    background:rgb(234,235,238);
    color:rgb(100,100,100);
    padding:5px 20px;
    font-weight:lighter;
    font-family:"Trajan Pro";

}
.new-assessment-container .build-your-assessment-container .assessment-questions-container .assessment-questions-body{
    min-height:5vh;
}

.new-assessment-main-container .new-assessment-footer{
    background:white;
    border:1px solid rgba(100,100,100,.7);
    position:fixed;
    width:100vw;
    top:100vh;
    transform:translateY(-100%);
}
.new-assessment-main-container .new-assessment-footer .new-assessment-previous-next-buttons {
    display:inline-block;
    position:relative;
    left:50%;
    transform:translateX(-50%);
    white-space:nowrap;
    padding:10px 20px;
}
.new-assessment-main-container .new-assessment-footer .new-assessment-previous-next-buttons div.next-btn .fa{
    margin-left:10px
}
.new-assessment-main-container .new-assessment-footer .new-assessment-previous-next-buttons div.back-btn .fa{
    margin-right:10px
}
.new-assessment-main-container .new-assessment-footer .new-assessment-previous-next-buttons div{
    display:inline-block;
    padding:5px 25px;
    box-shadow:0px 0px 0px 1px rgba(0,0,0,.4),inset 0px 1px rgba(255,255,255,.7);
    margin:0px 40px;
    border-radius:2px;
    font-family:"Trajan Pro";
    font-weight:bold;
    color:rgb(130,130,130);
    font-size:18px;
    cursor:pointer;
    transition:all 300ms;
}
.new-assessment-main-container .new-assessment-footer .new-assessment-previous-next-buttons div:not(.disabled):hover{
    background:rgb(240,240,240);
    color:rgb(100,100,100);
    box-shadow:0px 0px 2px 1px rgba(0,0,0,.2),inset 0px 0px rgba(255,255,255,.7);
}
.new-assessment-main-container .new-assessment-footer .new-assessment-previous-next-buttons div.disabled{
    background:rgb(210,210,210);
    box-shadow:0px 1px 1px rgba(0,0,0,.7);
    color:rgb(180,180,180);
    cursor:not-allowed;
}
@keyframes spin {
    0% { transform: rotate(0deg); }
    100% { transform: rotate(360deg); }
}

.loading-spinner {
    display:inline-block;
    content: '';
    box-sizing: border-box;
    position: absolute;
    top: 50%;
    left: 50%;

    width: 20px;
    height: 20px;
    margin-top: 0px;
    margin-left: 0px;
    border-radius: 50%;
    border-top: 5px solid #07d;
    border-right: 5px solid transparent;
    animation: spin 1s linear infinite;
}
.loading-splash-screen{
    background:rgba(0,0,0,.6);
    position:fixed;
    display:block;
    width:100vw;
    height:100vh;
    top:0;
    left:0;
    z-index:1000;
    opacity:1;
    transition-property: opacity, z-index;
    transition-duration: 500ms  , 1ms;
    transition-delay:    1ms    , 0ms;

}
.loading-spinner-container{
    text-align:center;
    display:inline-block;
    position:relative;
    left:50%;
    top:50%;
    transform:translate(-50%,-50%);
}
.loading-splash-screen .loading-spinner{
    width: 40vw;
    height: 40vw;
    left:0;
    top:0;
    border-width: 15px;


    position:relative;
}

.loading-splash-screen.disabled{
    opacity:0;
    z-index:-1;
    transition-property: opacity, z-index;
    transition-duration: 500ms  , 1ms;
    transition-delay:    1ms    , 501ms;

}
.loading-spinner-container  .loading-message {

    font-size:45px;
    font-family:"Adobe Caslon Pro";
    /*position:absolute;*/
    /*left:50%;*/
    /*top:50%;*/
    color:white;
    padding:10px;
    border-radius:10px;
    /*box-shadow:0px 1px 20px rgba(0,0,0,.9),inset 0px 1px rgba(255,255,255,.8);*.
    /*transform:translate(-50%,-50%);*/

}
.top-main-header {
    display:inline-block;
    width:100%;
    text-align:center;
    background:white;
    border:1px solid rgb(150,150,150,.8);
    position:relative;
}
.top-main-header .assessment-id{

    display:inline-block;
    padding:2px 20px;
    border:2px solid rgba(150,150,150,.5);
    left:5%;

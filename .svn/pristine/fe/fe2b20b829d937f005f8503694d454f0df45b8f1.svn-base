}
.import-results .results .entry-found{
    cursor:pointer;
    display: inline-block;

    padding: 5px;
    margin:2px;
    border:1px solid rgba(0, 0, 0, 0);
    /* border: 1px solid #000000; */
    border-radius:5px;
}
.import-results .results .entry-found:hover{
    background: rgba(205, 220, 57, 0.13);

    background:rgba(251, 251, 251, 0.98);
    margin:10px 0px;
}
.import-results .results-count:before{
    content:"Number of Entries found: ";
    font-weight:normal;
}
.import-results .results-count{
    display:inline-block;
    padding:10px;
    font-weight:bold;
    font-family:"Trajan Pro";
    font-size:20px;
}
.import-results .results{
    box-shadow:none;
    font-family:"Adobe Caslon Pro";
    font-size:18px;
    outline:none;
    border:none;
    border:1px solid rgba(0,0,0,.2);

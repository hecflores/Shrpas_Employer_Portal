
.outerborder{
  border: 1px solid blue;
  font-family:"Trajan Pro";
  color:rgb(102,102,102);
  background: rgb(184,184,184);
}
.outerborder .row{
  margin: 0px;
  border: 1px solid green;
}

.headerborder {
  
  margin: 10px;
  padding: 5px 10px;
  border: 1px solid green;
  font-size: 1.5em;
}
.MyTable .col-md-3.cell{
  border: 1px solid green;
  text-align: center;

}
.buttonstyle{
  -webkit-border-radius: 2;
  -moz-border-radius: 2;
  border-radius: 2px;
  font-family: Arial;
  color: #999999;
  font-size: 18px;
  background: #ffffff;
  padding: 6px 35px 8px 35px;
  border: solid #c2bec2 2px;
  text-decoration: none;
  font-family:"Trajan Pro";
  float: right;
  
}

.otherbuttonstyle{
   -webkit-border-radius: 2;

  background: #ffffff;
  padding: 6px 35px 8px 35px;
  border: solid #c2bec2 2px;
  text-decoration: none;
  font-family:"Trajan Pro";
  
}
.bordering{
  border: 1px solid red;
}
    


.footer {
  margin: 10px;
  border: 2px solid pink;
  text-align: center;
}
.MyTable{
  margin: 10px;
  border: 2px solid red;
  padding: 6px 35px 8px 35px;
  font-family: "Trajan Pro";
  text-align: center;
  
}
.MyTable .row{
  margin: 0px;
  color: #999999;
}
.container-header{
  border: 1px solid black;
  background: rgb(84,84,84);
